{
  "version": 2,
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "nodeVersion": "20.x"
      }
    }
  ],
  "images": {
    "domains": ["futuresolutionstestbed.eu"],
    "sizes": [16, 32, 48, 64, 96, 128, 256, 384],

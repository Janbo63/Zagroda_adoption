{
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "maxDuration": 60,
        "nodeVersion": "18.x"
      }
    }
  ],
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "continue": true
    }
  ]
} 